{
  "JWT_SECRET": "your_jwt_secret_here",
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "auth",
        "ClusterId": "authCluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      }
    ],
    "Clusters": [
      {
        "ClusterId": "authCluster",
        "Destinations": {
          "authService": {
            "Address": "http://localhost:5001/"
          }
        }
      }
    ]
  }
}
